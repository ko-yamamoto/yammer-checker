{
    "manifest_version": 2,
    "name": "Background Yammer Notifier",
    "version": "0.1",
    "description": "yammer の更新を通知します",
    "background": { "page": "background.html" },
    // "background": { "scripts": ["core.js"] },
    "permissions": [
        "notifications",
        "https://www.yammer.com/"
    ],
    "browser_action": {
        "default_icon": "Y-logo-300x300.png",
        "name": "Notification"
    }
}
