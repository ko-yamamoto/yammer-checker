{
    "manifest_version": 2,
    "name": "Yammer Notifier",
    "version": "0.5",
    "description": "yammer の更新を通知します",
    "background": { "page": "background.html" },
    "permissions": [
        "notifications",
        "https://www.yammer.com/"
    ],
    "browser_action": {
        "default_icon": "Y-logo-300x300.png",
        "name": "Notification"
    },
    "web_accessible_resources": [
        "Y-logo-300x300.png"
    ],
    "homepage_url": "https://github.com/nishikawasasaki/yammer-checker/",
    "options_page": "options.html"
}
